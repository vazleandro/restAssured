{
   "definitions": {},
   "$schema": "http://json-schema.org/draft-07/schema#",
   "$id": "http://example.com/root.json",
   "type": "object",
   "title": "The Root Schema",
   "required": [
     "data"
   ],
   "properties": {
     "data": {
       "$id": "#/properties/data",
       "type": "object",
       "title": "The Data Schema",
       "required": [
         "id",
         "type",
         "attributes"
       ],
       "properties": {
         "id": {
           "$id": "#/properties/data/properties/id",
           "type": "string",
           "title": "The Id Schema",
           "default": "",
           "examples": [
             "898"
           ],
           "pattern": "^(.*)$"
         },
         "type": {
           "$id": "#/properties/data/properties/type",
           "type": "string",
           "title": "The Type Schema",
           "default": "",
           "examples": [
             "contacts"
           ],
           "pattern": "^(.*)$"
         },
         "attributes": {
           "$id": "#/properties/data/properties/attributes",
           "type": "object",
           "title": "The Attributes Schema",
           "required": [
             "name",
             "last-name",
             "email",
             "age",
             "phone",
             "address",
             "state",
             "city"
           ],
           "properties": {
             "name": {
               "$id": "#/properties/data/properties/attributes/properties/name",
               "type": ["string","null"],
               "title": "The Name Schema",
               "default": "",
               "examples": [
                 "Fabiana Swdift"
               ],
               "pattern": "^(.*)$"
             },
             "last-name": {
               "$id": "#/properties/data/properties/attributes/properties/last-name",
               "type": ["string","null"],
               "title": "The Last-name Schema",
               "default": "",
               "examples": [
                 "Purdy"
               ],
               "pattern": "^(.*)$"
             },
             "email": {
               "$id": "#/properties/data/properties/attributes/properties/email",
               "type": "string",
               "title": "The Email Schema",
               "default": "",
               "examples": [
                 "donadldd.purdy@email.com"
               ],
               "pattern": "^(.*)$"
             },
             "age": {
               "$id": "#/properties/data/properties/attributes/properties/age",
               "type": "integer",
               "title": "The Age Schema",
               "default": 0,
               "examples": [
                 28
               ]
             },
             "phone": {
               "$id": "#/properties/data/properties/attributes/properties/phone",
               "type": "string",
               "title": "The Phone Schema",
               "default": "",
                 "examples": [
                   "2198475d9575"
                 ],
                 "pattern": "^(.*)$"
               },
               "address": {
                 "$id": "#/properties/data/properties/attributes/properties/address",
                 "type": ["string","null"],
                 "title": "The Address Schema",
                 "default": "",
                 "examples": [
                   "Rua Rio de Janeiro"
                 ],
                 "pattern": "^(.*)$"
               },
               "state": {
                 "$id": "#/properties/data/properties/attributes/properties/state",
                 "type": "string",
                 "title": "The State Schema",
                 "default": "",
                 "examples": [
                   "Minas Gerais"
                 ],
                 "pattern": "^(.*)$"
               },
               "city": {
                 "$id": "#/properties/data/properties/attributes/properties/city",
                 "type": ["string","null"],
                 "title": "The City Schema",
                 "default": "",
                 "examples": [
                   "Belo Horizonte"
                 ],
                 "pattern": "^(.*)$"
               }
             }
           }
         }
       }
     }
   }