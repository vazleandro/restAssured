{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "data"
  ],
  "properties": {
    "data": {
      "$id": "#/properties/data",
      "type": "array",
      "title": "The Data Schema",
      "items": {
        "$id": "#/properties/data/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "id",
          "type",
          "attributes"
        ],
        "properties": {
          "id": {
            "$id": "#/properties/data/items/properties/id",
            "type": "string",
            "title": "The Id Schema",
            "default": "",
            "examples": [
              "896"
            ],
            "pattern": "^(.*)$"
          },
          "type": {
            "$id": "#/properties/data/items/properties/type",
            "type": "string",
            "title": "The Type Schema",
            "default": "",
            "examples": [
              "contacts"
            ],
            "pattern": "^(.*)$"
          },
          "attributes": {
            "$id": "#/properties/data/items/properties/attributes",
            "type": "object",
            "title": "The Attributes Schema",
            "required": [
              "name",
              "last-name",
              "email",
              "age",
              "phone",
              "address",
              "state",
              "city"
            ],
            "properties": {
              "name": {
                "$id": "#/properties/data/items/properties/attributes/properties/name",
                "type": ["string","null"],
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "Fabiana11"
                ],
                "pattern": "^(.*)$"
              },
              "last-name": {
                "$id": "#/properties/data/items/properties/attributes/properties/last-name",
                "type": ["string","null"],
                "title": "The Last-name Schema",
                "default": null,
                "examples": [
                  null
                ]
              },
              "email": {
                "$id": "#/properties/data/items/properties/attributes/properties/email",
                "type": "string",
                "title": "The Email Schema",
                "default": "",
                "examples": [
                  "teste9189112@gmail.com"
                ],
                "pattern": "^(.*)$"
              },
              "age": {
                "$id": "#/properties/data/items/properties/attributes/properties/age",
                "type": "integer",
                "title": "The Age Schema",
                "default": 0,
                "examples": [
                  32
                ]
              },
              "phone": {
                "$id": "#/properties/data/items/properties/attributes/properties/phone",
                "type": "string",
                "title": "The Phone Schema",
                "default": "",
                "examples": [
                  "5519864666661"
                ],
                "pattern": "^(.*)$"
              },
              "address": {
                "$id": "#/properties/data/items/properties/attributes/properties/address",
                "type": ["string","null"],
                "title": "The Address Schema",
                "default": "",
                "examples": [
                  "Rua 4"
                ],
                "pattern": "^(.*)$"
              },
              "state": {
                "$id": "#/properties/data/items/properties/attributes/properties/state",
                "type": "string",
                "title": "The State Schema",
                "default": "",
                "examples": [
                  "Estado 4"
                ],
                "pattern": "^(.*)$"
              },
              "city": {
                "$id": "#/properties/data/items/properties/attributes/properties/city",
                "type": ["string","null"],
                "title": "The City Schema",
                "default": "",
                "examples": [
                  "Cidade 4"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        }
      }
    }
  }
}